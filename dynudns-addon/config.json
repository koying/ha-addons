{
  "name": "Dynu DNS",
  "version": "1.0.0",
  "slug": "dynudns",
  "description": "Free Dynamic DNS via Dynu with Let's Encrypt support",
  "url": "https://github.com/koying/ha-addons/tree/main/dynudns-addon",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "init": false,
  "hassio_api": true,
  "hassio_role": "default",
  "map": ["ssl:rw"],
  "options": {
    "lets_encrypt": {
      "accept_terms": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem"
    },
    "dns_api_token": null,
    "domains": [null],
    "aliases": [],
    "ip_update_wait_seconds": 300
  },
  "schema": {
    "lets_encrypt": {
      "accept_terms": "bool",
      "certfile": "str",
      "keyfile": "str"
    },
    "ipv4_fixed": "str?",
    "ipv6_fixed": "str?",
    "dns_api_token": "str",
    "domains": ["str"],
    "aliases": [
      {"domain": "str", "alias": "str"}
    ],
    "ip_update_wait_seconds": "int"
  }
}
